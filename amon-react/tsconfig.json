{
  "compilerOptions": {
    "downlevelIteration": true,
    "outDir": "dist",
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "noEmitOnError": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "utils/*": [
        "./utils/*"
      ],
      "components/*": [
        "./components/*"
      ]
    }
  },
  "include": [
    "**/*"
  ]
}